{
  "name": "git-chipper",
  "version": "0.5.0",
  "description": "An interactive CLI for batch deleting local branches.",
  "license": "MIT",
  "repository": "maxhallinan/git-chipper",
  "author": {
    "name": "Max Hallinan",
    "email": "maxhallinan@gmail.com",
    "url": "github.com/maxhallinan"
  },
  "bin": "cli.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "test": "xo && mocha test/**/*",
    "test-dev": "mocha test/unit/**/* --watch"
  },
  "files": [
    "cli.js",
    "git.js",
    "ui.js",
    "util.js"
  ],
  "keywords": [
    "cli-app",
    "cli",
    "git branch",
    "git delete"
  ],
  "dependencies": {
    "chalk": "^1.1.3",
    "inquirer": "^3.0.6",
    "meow": "^3.7.0",
    "simple-git": "^1.69.0",
    "update-notifier": "^2.1.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "xo": "^0.17.0"
  },
  "xo": {
    "esnext": true,
    "globals": [
      "before",
      "describe",
      "it"
    ],
    "rules": {
      "array-bracket-spacing": [
        "error",
        "always"
      ],
      "comma-dangle": [
        "error",
        "always"
      ],
      "unicorn/explicit-length-check": 0,
      "new-cap": [
        "error",
        {
          "capIsNew": false
        }
      ],
      "no-self-compare": 1,
      "object-curly-spacing": [
        "error",
        "always"
      ]
    },
    "space": 2
  }
}
